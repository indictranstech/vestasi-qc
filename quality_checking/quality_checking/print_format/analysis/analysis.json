 "html": " \n<html>\n     <head>\n      <style>\n        @media print {\n           html, body {\n                      width: 100%;\n                      height: 100%;\n                      margin: 0;\n                      padding: 0;\n           }\n\n           div.footer{\n                     position: absolute;\n                     bottom: 0;\n                        \n           }\n\n           .page_break {\n                     page-break-after: always;\n           }\n          }\n</style>\n    \n     </head>\n     <body>\n          <table>\n                <tr>\n                    <td align='left'><img src=\"/files/VSI-Logo-Color-Tag-Small.jpg\"></td>\n                </tr>\n                <tr>\n                    <td><strong>Subsidiary of Vesta Si LLC</strong></td>\n                </tr>\n                <tr>\n                </tr>\n                <tr>\n                   <td><h5><Strong>ANALYTICAL CERTIFICATE&nbsp{{ doc.name }}<br>{{ doc.date }}, {{   doc.customer }}</strong></h5></td>\n                </tr>\n                <tr>\n                </tr>\n                <tr>\n                   <td>{{ doc.item_code }} &nbsp {{ doc.total_qty }} Kg, {{ doc.total_drums }} Drums </td>\n                 </tr>\n                <tr>\n                   <td>DRUM NO:{{ doc.series }} </td>\n                </tr>\n                <tr>\n                </tr>\n                <tr>\n                  <td>The chemical content and particle size distribution was found to be:</td>\n                </tr>\n          </table>\n          <table width=100% border=\"1\">\n                 <tr align='center'>\n                    {% set fields=doc.mandatory_fields.split(',') %}\n                    {% if fields[0] %}\n                        <th>SrNo</th>\n                          <th>Drum</th>\n                          <th>Weight</th>\n                          {% for f in fields -%}\n                              <th>{{ f }}</th>\n                          {%- endfor -%} \n                 </tr>\n                 {% for item in doc.qa_serial -%}\n                 <tr>\n                      <td>{{ item.idx }}</td>\n                      <td>{{ item.serial_no }}</td>\n                      <td>{{ item.qty }}</td>\n                      {% for f in fields -%}\n                       <td>{{ item.get(f) }}</td>\n                      {%- endfor -%}\n                 </tr>\n                 {%- endfor -%}\n               {% else %}\n                  <Strong><b><h5>OOP'S Item you have selected does not have Mandatory parameters!<h5></b></strong>\n               {%- endif -%}      \n          </table>\n          <br><br>\n          <table>\n                 <tr width=100%>\n                     <td>Particle size distribution: Micromeretics Saturn DigiSizer 5200 <br>\n                     The product is analyzed with a sampling frequency required for statistical quality assurance.\n                     </td>\n                 </tr>\n                 <tr></tr>\n                 <tr></tr>\n           </table>\n           <table width=100%>\n                 <tr>\n                  <td width=50%><Strong>Which is hereby certified</strong><br> Ljungaverk, {{ doc.date }}</td>\n                 </tr>\n                 <tr><td width=50%></td><td></td></tr>\n                 <tr><td width=50%></td><td></td></tr>\n                 <tr><td width=50%></td><td></td></tr>\n                 <tr>\n                      <td style=\"border-bottom: dotted 1px;\" width=50%></td><td></td>\n                 </tr>\n                 <tr><td width=50%>{{ doc.operator_name }}&nbsp{{ doc.operator_last }}</td></tr>\n          </table>\n           <div class='footer' width=100%>\n                <hr width=100%>\n                <table>\n                       <tr width=100%>\n                            <td width=30% align=\"left\"><img src=\"/files/BV_Certification_ISO_9001.jpg\"></td> \n                            <td width=30%></td>\n                            